/* pages/setting/setting.wxss */
.left {
	width: 100%;
	height: 600rpx;
	display: flex;
	justify-content: center;
	align-items: center;
    position: relative;
    margin: 0 auto;
}
 
.white {
	width: 0;
	height: 0;
	border-radius: 50%;
	position: absolute;
	top: 50%;
	left: 50%;
	z-index: 10;
	background-color: #fff;
	transform: translate(-50%, -50%);
}